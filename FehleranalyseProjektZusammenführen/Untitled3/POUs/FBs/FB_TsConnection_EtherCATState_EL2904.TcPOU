    nEtherCATState : UINT; // EtherCAT Slave State
END_VAR
VAR_OUTPUT
    sEtherCATState : STRING;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[CASE nEtherCATState OF
    16#0001: sEtherCATState := 'INIT';
    16#0002: sEtherCATState := 'PREOP';
    16#0003: sEtherCATState := 'BOOT';
    16#0004: sEtherCATState := 'SAFEOP';
    16#0008: sEtherCATState := 'OP';
ELSE
    sEtherCATState := 'undefined';
END_CASE]]></ST>
